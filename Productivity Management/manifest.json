{
    "manifest_version": 2,
    "name": "Productivity-Manager",
    "version": "0.1",
    "background": {
        "scripts": ["./public/jquery-3.6.0.min.js","./public/background.js"]
    },
    "browser_action": {
        "default_popup": "./popup.html",
        "default_icon": "icon.png"
    },    
    "permissions": ["<all_urls>","storage","activeTab","notifications","webRequest", "webRequestBlocking"]
}